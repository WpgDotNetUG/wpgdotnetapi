<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <remove name="httpplatformhandler" />
      <add name="httpplatformhandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
    </handlers>
    <httpPlatform
              stdoutLogEnabled="false" stdoutLogFile="fake.log" startupTimeLimit="20"
              processPath="%HOME%\site\wwwroot\packages\FAKE\tools\FAKE.exe"
              arguments="%HOME%\site\wwwroot\app.azure.fsx port=%HTTP_PLATFORM_PORT%" >
      <environmentVariables>
        <environmentVariable name="WhateverYouLike" value="GoesHere"/>
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
  <appSettings>
    <add key="SlackToken" value="FILL IN WITH REAL SLACK TOKEN/>
    <add key="SlackOrg" value="wpgdotnet"/>
  </appSettings>
</configuration>
